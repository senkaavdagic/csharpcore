@page
@using Library
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model MyFeatures.Pages.EmployeesPageModel
<div class="row">
    <h2 class="display-2"> </h2>
</div>
<div class="row">
    @foreach(Employee employee in Model.Employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="employee" />
        </div>
    }
</div>